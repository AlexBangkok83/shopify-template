# Shopify Configuration
# Copy this file to .env and fill in your actual values

# Brand 1 - Premium Fashion
BRAND1_SHOP_DOMAIN=your-premium-shop.myshopify.com
BRAND1_STOREFRONT_ACCESS_TOKEN=your_storefront_access_token_here

# Brand 2 - Eco Essentials  
BRAND2_SHOP_DOMAIN=your-eco-shop.myshopify.com
BRAND2_STOREFRONT_ACCESS_TOKEN=your_eco_storefront_access_token_here

# Brand 3 - Tech Gadgets
BRAND3_SHOP_DOMAIN=your-tech-shop.myshopify.com
BRAND3_STOREFRONT_ACCESS_TOKEN=your_tech_storefront_access_token_here

# API Configuration
SHOPIFY_API_VERSION=2024-01

# Analytics (Optional)
GOOGLE_ANALYTICS_ID=
FACEBOOK_PIXEL_ID=
TIKTOK_PIXEL_ID=

# Development
NODE_ENV=development
PORT=3000